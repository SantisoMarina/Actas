
@{
    ViewBag.Title = "_Persona";
}

@using (Ajax.BeginForm("PersonasResultado", "Acta", new AjaxOptions
{
    InsertionMode = InsertionMode.Replace,
    HttpMethod = "POST",
    UpdateTargetId = "tablaPersona"

}))
{
    <p>
        Buscar por número de documento: @Html.TextBox("searchString")
        <input type="submit" id="search" class="btn btn-success btn-xs" value="Buscar" />
    </p>
}


<div id="tablaPersona">
</div>

